import vertexai
from vertexai.generative_models import GenerativeModel, Part, Image

# Initialize Vertex AI
# TODO: Replace with your actual project ID and location
vertexai.init(project="your-project-id", location="us-central1")

def generate_multimodal_content(image_path, prompt_text):
    """
    Generates content using Gemini Pro Vision with text and image inputs.
    """
    model = GenerativeModel("gemini-1.0-pro-vision")
    
    # Load image (assuming local or GCS URI)
    image = Part.from_uri(uri=image_path, mime_type="image/jpeg")
    
    responses = model.generate_content(
        [image, prompt_text],
        generation_config={
            "max_output_tokens": 2048,
            "temperature": 0.4,
            "top_p": 1,
            "top_k": 32
        }
    )
    
    return responses.text

